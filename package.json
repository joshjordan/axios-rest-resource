    "@types/moxios": "^0.4.9",
    "@types/node": "^14.0.4",
    "axios": "^1.2.0",
    "doctoc": "^2.2.0",
    "husky": "^1.3.1",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.3",
    "moxios": "^0.4.0",
    "prettier": "^1.16.1",
